            "LunaTranslator"
        ],
        [
            "LunaTranslator_admin.exe",
            "LunaTranslator (Admin)"
        ]
    ],
    "persist": "userconfig",
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "32bit": {
