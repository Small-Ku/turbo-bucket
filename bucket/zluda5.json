{
    "version": "v5-preview.109",
    "description": "CUDA on AMD GPUs.",
    "homepage": "https://github.com/vosen/ZLUDA",
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://github.com/vosen/ZLUDA/blob/master/LICENSE-APACHE"
    },
    "url": "https://github.com/vosen/ZLUDA/releases/download/v5-preview.109/zluda-windows-8a7a5b4.zip",
    "hash": "374ce90a6b434f2ea0b0d41cc2b6fe48f6ff860c201b04f5ecc364a1952de6e0",
    "bin": "zluda_with.exe",
    "env_add_path": ".",
    "checkver": {
        "script": [
            "$rel_list = Invoke-RestMethod -Uri 'https://api.github.com/repos/vosen/ZLUDA/releases'",
            "$rel = $rel_list | Sort-Object {$_.published_at} | Select-Object -last 1",
            "$download_url = ($rel.assets | Where-Object { $_.name -match 'windows' } | Select-Object -first 1).browser_download_url",
            "Write-Output $rel.tag_name $download_url"
        ],
        "regex": "(?<version>[\\w\\d.-]+)\\s(?<url>[^\\s]+)"
    },
    "installer": {
        "script": [
            "if (Test-Path \"$dir\\zluda\") {",
            "Move-Item \"$dir\\zluda\\*\" \"$dir\"",
            "Remove-Item -Recurse \"$dir\\zluda\"",
            "}"
        ]
    },
    "autoupdate": {
        "url": "$matchUrl"
    }
}
