        }
    },
    "shortcuts": [
        [
            "Icalingua++.exe",
            "Icalingua++"
        ]
    ],
    "checkver": {
        "script": [
            "$actions = Invoke-RestMethod -Uri 'https://api.github.com/repos/Icalingua-plus-plus/Icalingua-plus-plus/actions/workflows/main.yml/runs'",
            "$latest_develop = $actions.PSObject.Properties.Value | Where-Object { $_.head_branch -eq 'develop' } | Where-Object { $_.conclusion -eq 'success' } | Select-Object -first 1",
            "$latest_stable_version = ($actions.PSObject.Properties.Value | Where-Object { $_.display_title -match 'release: v\\d+\\.\\d+\\.\\d+' } | Where-Object { $_.conclusion -eq 'success' } | Select-Object -first 1).head_branch.TrimStart('v')",
            "$version = $latest_stable_version + '-' + $latest_develop.head_sha.substring(0,8)",
            "Write-Output $version $latest_develop.id"
        ],
        "regex": "(?<version>[^\\s]+)\\s(?<id>\\d+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://nightly.link/Icalingua-plus-plus/Icalingua-plus-plus/actions/runs/$matchId/Icalingua_Windows64.exe.zip",
                "pre_install": [
                    "Expand-7zipArchive \"$dir\\Icalingua++-$matchVersion_x64.exe\" \"$dir\"",
                    "Remove-Item \"$dir\\Icalingua++-$matchVersion_x64.exe\" -Force -Recurse",
                    "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                    "Remove-Item \"$dir\\`$*\", \"$dir\\Uninst*\" -Force -Recurse"
                ]
            },
            "arm64": {
                "url": "https://nightly.link/Icalingua-plus-plus/Icalingua-plus-plus/actions/runs/$matchId/Icalingua_Windows_arm64.exe.zip",
                "pre_install": [
                    "Expand-7zipArchive \"$dir\\Icalingua++-$matchVersion_arm64.exe\" \"$dir\"",
                    "Remove-Item \"$dir\\Icalingua++-$matchVersion_arm64.exe\" -Force -Recurse",
                    "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-arm64.7z\" \"$dir\"",
                    "Remove-Item \"$dir\\`$*\", \"$dir\\Uninst*\" -Force -Recurse"
                ]
            }
        }
    }
}
