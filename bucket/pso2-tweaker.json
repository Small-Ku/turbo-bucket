        [
            "PHANTASYSTARONLINE2_NA",
            "game_na"
        ],
        [
            "PHANTASYSTARONLINE2_NA_STEAM",
            "game_na_steam"
        ],
        [
            "PHANTASYSTARONLINE2_NA_EPICGAMES",
            "game_na_epic"
        ]
    ]
