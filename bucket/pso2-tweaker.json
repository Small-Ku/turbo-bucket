{
    "version": "6.2.0.0",
    "description": "A replacement for the normal PSO2 launcher, used to update, patch, and launch the Japanese and Global versions of the game.",
    "homepage": "https://arks-layer.com/",
    "license": {
        "identifier": "Unknown"
    },
    "url": "https://github.com/Aida-Enna/PSO2TweakerReleases/blob/master/6.2.0.0/PSO2 Tweaker.exe?raw=true",
    "hash": "a4df509658dc24e64d8a05b8139b01d18c2175e8f8880a5d6ffe6c9bf20526fa",
    "checkver": {
        "url": "https://arks-layer.com/vanila/changelog.html",
        "regex": "<h3>(?<version>\\d\\.\\d\\.\\d\\.\\d)\\s*\\(\\d+-\\d+-\\d{4,}\\)</h3>",
        "replace": "${version}"
