    "homepage": "https://arks-layer.com/",
    "license": {
        "identifier": "Unknown"
    },
    "url": "https://github.com/Aida-Enna/PSO2TweakerReleases/blob/master/6.2.1.1/PSO2 Tweaker.exe?raw=true",
    "hash": "570c989dc863ecead06b089841b9368b7acb21142aa73cf90b216701aae0368e",
