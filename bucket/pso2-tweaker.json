    },
    "bin": "PSO2 Tweaker.exe",
    "shortcuts": [
        [
            "PSO2 Tweaker.exe",
            "PSO2 Tweaker"
        ]
    ],
    "persist": [
        [
            "PHANTASYSTARONLINE2_JP",
            "game_jp"
        ],
        [
            "PHANTASYSTARONLINE2_NA",
            "game_na"
        ],
        [
            "PHANTASYSTARONLINE2_NA_STEAM",
            "game_na_steam"
        ],
        [
            "PHANTASYSTARONLINE2_NA_EPICGAMES",
            "game_na_epic"
        ]
    ]
